<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.assetnest.assetnest_frontend_android">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Starter"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Assetnest">

        <activity android:name=".modul.splash.SplashActivity" android:theme="@style/Theme.Assetnest.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".modul.login.LoginActivity" android:theme="@style/Theme.Assetnest" />
        <activity android:name=".modul.home.HomeActivity" android:theme="@style/Theme.Assetnest" />
        <activity android:name=".modul.profile.ProfileActivity" android:theme="@style/Theme.Assetnest" />
        <activity android:name=".modul.asset.AssetActivity" android:theme="@style/Theme.Assetnest" />
        <activity android:name=".modul.history.HistoryActivity" android:theme="@style/Theme.Assetnest" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>
</manifest>